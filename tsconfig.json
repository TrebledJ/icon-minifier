{
    "compilerOptions": {
        "lib": ["es6", "dom"],
        "target": "es2017",
        "module": "esnext",
        "moduleResolution": "Bundler",
        "rootDir": ".",
        "outDir": "dist",
        "strict": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "downlevelIteration": true,
    },
    "include": [
        "src/**/*.ts",
        "test/**/*.ts"],
    "exclude": [
        "node_modules"
    ],
    "extends": "./node_modules/gts/tsconfig-google.json",
}